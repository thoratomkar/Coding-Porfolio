    "def myfunc(s):\n",
    "    mylist = []\n",
    "    for x in range(0,len(s)):\n",
    "        if x%2==0:\n",
    "            mylist.append(s[x].upper())\n",
    "        else:\n",
    "            mylist.append(s[x].lower())\n",
    "    return ''.join(mylist)\n",
    "    \n",
    "result=myfunc('vhasdvhgsaddfsgddgdhdhd')\n",
    "print(result)"
